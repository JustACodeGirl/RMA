body {
    width: 1170px;
    height: auto;
    margin: 0 auto;
    min-height: 100%;
    font-family: "Microsoft Yahei", "proxima-nova", Arial, sans-serif;
    -webkit-font-smoothing: antialiased;
    font-weight: 400;
    overflow-x: auto;
    box-shadow: 0 0 2px 2px #cccccc;
}

html {
    position: relative;
    width: 100%;
    height: 100%;
    background-color: #f7f7f7;
}

p {
    font-size: 1.1em;
}

.wrapper {
    padding-left: 50px;
    padding-right: 50px;
}
.wrapper-sm {
    padding-left: 30px;
    padding-right: 30px;
}
.container {
    width: 1170px !important;
}

#navbar ul li a:hover {
    color: #00C1F3;
}

.navbar {
    background: transparent;
    background-color: transparent;
    padding: 10px 0 0;
    box-shadow: none;
    border: none;
    font-weight: 400;
    -webkit-transition: background .5s ease-in-out, padding .5s ease-in-out;
    -moz-transition: background .5s ease-in-out, padding .5s ease-in-out;
    -ms-transition: background .5s ease-in-out, padding .5s ease-in-out;
    transition: background .5s ease-in-out, padding .5s ease-in-out;
}

.navbar-default .navbar-brand {
    color: #000;
    line-height: 42px;
}

.navbar-default .navbar-nav > li > a {
    color: #000;
    font-weight: 400;
    font-size: 1.2em;
    letter-spacing: 0.080em;
    line-height: 42px;
    padding: 10px 0;
}

.banner {
    min-height: 500px;
    background-image: url('../img/video_bg.jpg');
}

.section {
    min-height: 500px;
}

.no-border {
    border: none;
}

.card {
    padding-top: 20px;
    height: 360px;
    border-radius: 4px;
}

.card p{
    line-height: 1.5em;
    margin-top: 20px;
}

.footer {
    background-color: #3a3a3a;
}

.text-white {
    color: #FFFFFF;
}

.navbar-semi-invisible {
    background-color: rgb(255, 255, 255);
    border-bottom: none;
}

#myMenu li {
    width: 115px;
}

#myMenu li:first-child {
}

#myMenu li:nth-child(4):after {
    content: "";
    width: 115px;
    height: 2px;
    position: absolute;
    background: #00C1F3;
    bottom: 0;
    left: 0;
    transform: translateX(-345px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.specs li:nth-child(4):after {
    transform: translateX(-230px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.instance li:nth-child(4):after {
    transform: translateX(-115px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.faqs li:nth-child(4):after {
    transform: translateX(0);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.specs li:nth-child(1):hover ~ li:nth-child(4):after,
#myMenu.instance li:nth-child(1):hover ~ li:nth-child(4):after,
#myMenu.faqs li:nth-child(1):hover ~ li:nth-child(4):after {
    transform: translateX(-345px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu li:nth-child(4):hover:after {
    transform: translateX(0);
}

#myMenu li:nth-child(3):hover ~ li:nth-child(4):after {
    transform: translateX(-115px);
}

#myMenu li:nth-child(2):hover ~ li:nth-child(4):after {
    transform: translateX(-230px);
}

#myMenu li:nth-child(1):hover ~ li:nth-child(4):after {
    transform: translateX(-345px);
}

#myMenu li a {
    text-decoration: none;
    color: #2c3e50;
    cursor: pointer;
}

.btn-buy {
    color: #FFF;
    background-color: #00C1F3;
    padding: 9px 20px;
