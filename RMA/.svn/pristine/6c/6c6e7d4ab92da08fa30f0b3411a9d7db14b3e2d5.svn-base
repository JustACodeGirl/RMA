@media (max-width: 768px) {
    .widget-hide {
        display: none;
    }

    #myMenu li {
        width: 100%;
    }

    #logo {
        width: 120px;
        margin-top: -23px;
    }

    .coming img {
        width: 80%;
        max-width: 432px;
    }

    .navbar {
        background: #FFFFFF;
        padding: 0;
    }

    .vjs_video_3-dimensions {
        width: 100% !important;
    }
}

@media (min-width:768px) and (max-width: 1000px) {
    #myMenu li {
        width: 100px;
    }

    #logo {
        width: 120px;
    }
    .row {
        margin-left: 0;
        margin-right: 0;
    }

    .coming img {
        width: 60%;
        max-width: 432px;
    }

    #myMenu li:nth-child(4):after {
        width: 100px;
        transform: translateX(-300px);
    }

    #myMenu.specs li:nth-child(4):after {
        transform: translateX(-200px);
    }

    #myMenu.instance li:nth-child(4):after {
        transform: translateX(-100px);
    }

    #myMenu.faqs li:nth-child(4):after {
        transform: translateX(0);
    }

    #myMenu.specs li:nth-child(1):hover ~ li:nth-child(4):after,
    #myMenu.instance li:nth-child(1):hover ~ li:nth-child(4):after,
    #myMenu.faqs li:nth-child(1):hover ~ li:nth-child(4):after {
        transform: translateX(-300px);
    }

    #myMenu li:nth-child(4):hover:after {
        transform: translateX(0);
    }

    #myMenu li:nth-child(3):hover ~ li:nth-child(4):after {
        transform: translateX(-100px);
    }

    #myMenu li:nth-child(2):hover ~ li:nth-child(4):after {
        transform: translateX(-200px);
    }

    #myMenu li:nth-child(1):hover ~ li:nth-child(4):after {
        transform: translateX(-300px);
    }
}

a.video {
    display: block;
    cursor: pointer;
    max-width: 240px;
    background-image: url("../img/play.png");
    background-size: contain;
    background-repeat: no-repeat;
    background-position-x: 50px;
    transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
}

a.video img {
    width: 100%;
    display: block;
    transition: all 0.3s ease-out;
    -webkit-transition: all 0.3s ease-out;
}

a.video:hover {
    box-shadow: 1px 1px 2px 2px #bbbdbc;
}

a.video:hover img {
    opacity: 0.3;
}

