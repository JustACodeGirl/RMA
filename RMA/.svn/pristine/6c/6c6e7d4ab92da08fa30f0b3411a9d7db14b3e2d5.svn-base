body {
    width: 100%;
    height: 100%;
    font-family: "Microsoft Yahei", "proxima-nova", Arial, sans-serif;
    -webkit-font-smoothing: antialiased;
    font-weight: 400;
    overflow-x: hidden;
}

html {
    position: relative;
    width: 100%;
    height: 100%;
}

h1 {
    font-size: 2.8em;
    font-weight: 400;
    padding-top: 20px;
    padding-bottom: 20px;
    color: #000000;
}

p {
    font-size: 1.5em;
    padding-top: 10px;
    padding-bottom: 10px;
    color: #535456;
}

.row {
    margin-left: 0;
    margin-right: 0;
}


#navbar ul li a:hover {
    color: #00C1F3;
}

.navbar {
    background: transparent;
    background-color: transparent;
    padding: 20px 0 0;
    box-shadow: none;
    border: none;
    font-weight: 400;
    -webkit-transition: background .5s ease-in-out, padding .5s ease-in-out;
    -moz-transition: background .5s ease-in-out, padding .5s ease-in-out;
    -ms-transition: background .5s ease-in-out, padding .5s ease-in-out;
    transition: background .5s ease-in-out, padding .5s ease-in-out;
}

.navbar-default .navbar-brand {
    color: #000;
    line-height: 42px;
}

.navbar-default .navbar-nav > li > a {
    color: #000;
    font-weight: 400;
    font-size: 1.2em;
    letter-spacing: 0.080em;
    line-height: 42px;
    padding: 10px 0;
}

.breadcrumb {
    margin: 0 auto;
    background-color: transparent;
}

.breadcrumb > li + li:before {
    content: "|";
}

.breadcrumb > li > a {
    color: #000000;
    text-decoration: none;
}

.breadcrumb > li > a:hover {
    color: #00C1F3;
}

.breadcrumb > .active > a {
    color: #00C1F3;
}

.navbar-semi-invisible {
    background-color: rgb(255, 255, 255);
    border-bottom: none;
}

#myMenu li {
    width: 115px;
}

#myMenu li:first-child {
}

#myMenu li:nth-child(4):after {
    content: "";
    width: 115px;
    height: 2px;
    position: absolute;
    background: #00C1F3;
    bottom: 0;
    left: 0;
    transform: translateX(-345px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.specs li:nth-child(4):after {
    transform: translateX(-230px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.instance li:nth-child(4):after {
    transform: translateX(-115px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.faqs li:nth-child(4):after {
    transform: translateX(0);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu.specs li:nth-child(1):hover ~ li:nth-child(4):after,
#myMenu.instance li:nth-child(1):hover ~ li:nth-child(4):after,
#myMenu.faqs li:nth-child(1):hover ~ li:nth-child(4):after {
    transform: translateX(-345px);
    -webkit-transition: .5s;
    -moz-transition: .5s;
    -ms-transition: .5s;
    transition: .5s;
}

#myMenu li:nth-child(4):hover:after {
    transform: translateX(0);
}

#myMenu li:nth-child(3):hover ~ li:nth-child(4):after {
    transform: translateX(-115px);
}

#myMenu li:nth-child(2):hover ~ li:nth-child(4):after {
    transform: translateX(-230px);
}

#myMenu li:nth-child(1):hover ~ li:nth-child(4):after {
    transform: translateX(-345px);
}

#myMenu li a {
    text-decoration: none;
    color: #2c3e50;
    cursor: pointer;
}

